{
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.import.maven.enabled": true,
    "java.project.sourcePaths": [
        "src/main/java",
        "src/test/java"
    ],
    "java.project.outputPath": "target/classes",
    "java.project.referencedLibraries": [
        "lib/**/*.jar"
    ],
    "java.test.config": {
        "workingDirectory": "${workspaceFolder}"
    },
    "coverage-gutters.coverageFileNames": ["target/site/jacoco/jacoco.xml", "jacoco.xml"],
    "coverage-gutters.showLineCoverage": true,
    "coverage-gutters.ignoredPathGlobs": [
        "**/test/**",
        "**/src/test/**",
        "**/test/java/**",
        "**/*Test.java",
        "**/*Tests.java",
        "**/Test*.java"
    ],
    "coverage-gutters.lcovname": "lcov.info",
    "coverage-gutters.watch": true,
    "coverage-gutters.coverageType": "gutter",
    "coverage-gutters.fullCoverageDecorationType": "gutter",
    "coverage-gutters.partialCoverageDecorationType": "gutter",
    "coverage-gutters.noCoverageDecorationType": "gutter",
    "java.clean.workspace": false,
    "java.compile.nullAnalysis.mode": "automatic"
}
